boot_mnt=\/dev\/block\/mmcblk0p20
data_mnt=\/dev\/block\/mmcblk0p29
sdcard_mnt=\/dev\/block\/mmcblk1p1

modem_mnt=\/dev\/block\/mmcblk0p2

fix_boot2=yes

