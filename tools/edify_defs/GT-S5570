
# Samsung Galaxy Mini

change_mnt=yes

param1=ext4
param2=EMMC

param1_sdcard=vfat
param2_sdcard=MTD

sys_mnt=\/dev\/block\/mmcblk0p9
cache_mnt=\/dev\/block\/mmcblk0p7
boot_mnt=\/dev\/block\/mmcblk0p5
data_mnt=\/dev\/block\/mmcblk0p10
sdcard_mnt=\/dev\/block\/mmcblk0p11

modem_mnt=\/dev\/block\/mmcblk0p8

