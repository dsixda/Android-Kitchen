
# Samsung Galaxy S Advance

change_mnt=yes

param1=ext4
param2=EMMC

param1_sdcard=vfat

sys_mnt=\/dev\/block\/mmcblk0p3
cache_mnt=\/dev\/block\/mmcblk0p4
boot_mnt=\/dev\/block\/mmcblk0p15
data_mnt=\/dev\/block\/mmcblk0p5
sdcard_mnt=\/dev\/block\/mmcblk0p8

modem_mnt=\/dev\/block\/mmcblk0p2

preload_mnt=\/dev\/block\/mmcblk0p9

fix_boot2=yes


